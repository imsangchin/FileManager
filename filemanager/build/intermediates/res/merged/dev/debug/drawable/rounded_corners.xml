<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@+id/shape_id"
        android:top="4dp"
        android:bottom="4dp"
        android:left="2dp"
        android:right="2dp" >
        <shape android:shape="rectangle" >
            <corners android:radius="20dp" />
            <solid android:color="#808080" />
            <padding
                android:top="8dp"
                android:left="8dp"
                android:right="8dp"
                android:bottom="8dp" />
        </shape>
    </item>
</layer-list>